{
  "name": "react-starter",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve": "webpack -c build/webpack.prod.js",
    "dev": "cross-env NODE_ENV=development APP_ENV=development webpack-dev-server -c build/webpack.dev.js",
    "dev:test": "cross-env NODE_ENV=development APP_ENV=test webpack-dev-server -c build/webpack.dev.js",
    "dev:prod": "cross-env NODE_ENV=development APP_ENV=production webpack-dev-server -c build/webpack.dev.js",
    "build:dev": "cross-env NODE_ENV=production APP_ENV=development webpack -c build/webpack.prod.js",
    "build:test": "cross-env NODE_ENV=production APP_ENV=test webpack -c build/webpack.prod.js",
    "build:prod": "cross-env NODE_ENV=production APP_ENV=production webpack -c build/webpack.prod.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^9.1.2",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}